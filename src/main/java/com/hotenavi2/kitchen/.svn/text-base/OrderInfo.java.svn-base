package com.hotenavi2.kitchen;

/**
 * 伝票情報クラス
 * 
 * @author Y.Tanabe
 * @version 2.00 2011/07/11
 */
public class OrderInfo
{
    // 伝票番号
    public int    SlipNo;
    // 受付日時
    public String AcceptDatetime;
    // 配膳日時
    public String SettableDatetime;
    // 部屋コード
    public int    RoomCode;
    // 部屋名称
    public String RoomName;
    // オーダー分類コード
    public int    OrderClassCode;
    // オーダー分類名称
    public String OrderClassName;
    // 商品コード
    public int    GoodsCode;
    // 商品名称
    public String GoodsName;
    // 数量
    public int    GoodsCount;
    // 配膳完了日時
    public String SettableFinDatetime;
}
