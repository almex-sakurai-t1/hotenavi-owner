package jp.happyhotel.reserve;

import java.util.ArrayList;

/**
 * 必須オプション用Formクラス
 */
public class FormReserveOptionSubImp
{

    private ArrayList<Integer> optIdList    = new ArrayList<Integer>();
    private ArrayList<String>  optNmList    = new ArrayList<String>();
    private ArrayList<Integer> optSubIdList = new ArrayList<Integer>(); // 数量のリスト
    private ArrayList<String>  optSubNmList = new ArrayList<String>();

    public ArrayList<Integer> getOptIdList()
    {
        return optIdList;
    }

    public void setOptIdList(ArrayList<Integer> optIdList)
    {
        this.optIdList = optIdList;
    }

    public ArrayList<String> getOptNmList()
    {
        return optNmList;
    }

    public void setOptNmList(ArrayList<String> optNmList)
    {
        this.optNmList = optNmList;
    }

    public ArrayList<Integer> getOptSubIdList()
    {
        return optSubIdList;
    }

    public void setOptSubIdList(ArrayList<Integer> optSubIdList)
    {
        this.optSubIdList = optSubIdList;
    }

    public ArrayList<String> getOptSubNmList()
    {
        return optSubNmList;
    }

    public void setOptSubNmList(ArrayList<String> optSubNmList)
    {
        this.optSubNmList = optSubNmList;
    }
}
