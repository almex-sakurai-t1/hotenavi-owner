package jp.happyhotel.data;

public class DataSearchReserve
{

    private int    id           = 0;
    private String hotel_name   = "";
    private String reserve_no   = "";
    private int    reserve_date = 0;
    private String reserve_dt   = "";
    private int[]  reserveCnt   = null;

    /**
     * getter
     */
    public int getId()
    {
        return id;
    }

    public String getHotelName()
    {
        return hotel_name;
    }

    public String getReserveNo()
    {
        return reserve_no;
    }

    public int getReserveDate()
    {
        return reserve_date;
    }

    public String getReserveDt()
    {
        return reserve_dt;
    }

    public int[] getReserveCount()
    {
        return reserveCnt;
    }

    /**
     * setter
     */
    public void setId(int id)
    {
        this.id = id;
    }

    public void setHotelName(String hotelnm)
    {
        this.hotel_name = hotelnm;
    }

    public void setReserveNo(String reserveno)
    {
        this.reserve_no = reserveno;
    }

    public void setReserveDate(int reservedate)
    {
        this.reserve_date = reservedate;
    }

    public void setReserveDt(String reservedt)
    {
        this.reserve_dt = reservedt;
    }

    public void setReserveCount(int[] reservecount)
    {
        this.reserveCnt = reservecount;
    }


}
