package jp.happyhotel.dto;

import java.io.Serializable;

/**
 * ホテル顧客データ
 * 
 * @author Shingo Tashiro
 * @version 1.00 2014/8/17
 */
public class DtoApHotelCustomer implements Serializable
{

    /**
     *
     */
    private static final long serialVersionUID = -5341943795189619658L;

    private boolean           resultMemberInfo;                        // メンバー情報取得結果
    private boolean           resultMemberCard;                        // メンバー受付情報結果
    private boolean           resultMemberCheckIn;                     // メンバーチェックイン結果
    private String            customId;                                // ホテル会員ID
    private String            rank;                                    // ランク
    private int               point;                                   // ポイント
    private int               point2;                                  // ポイント2
    private boolean           existMemberInfo;                         // 詳細情報有無
    private String            errorMessage;                            // エラーメッセージ
    private int               errorCode;                               // エラーコード
    private DtoApCommon       apCommon;                                // タッチ共通データ
    private boolean           resultMemberCardIssued;                  // メンバーカード発行有無
    private int               goodsCode;                               // 商品コード
    private int               goodsPrice;                              // 金額

    /**
     * データを初期化します。
     */
    public DtoApHotelCustomer()
    {
        this.resultMemberInfo = false;
        this.resultMemberCard = false;
        this.resultMemberCheckIn = false;
        this.customId = "";
        this.rank = "";
        this.point = 0;
        this.point2 = 0;
        this.existMemberInfo = false;
        this.errorMessage = "";
        this.errorCode = 0;
        this.apCommon = null;
        this.resultMemberCardIssued = false;
        this.goodsCode = 0;
        this.goodsPrice = 0;
    }

    public boolean isResultMemberInfo()
    {
        return resultMemberInfo;
    }

    public void setResultMemberInfo(boolean resultMemberInfo)
    {
        this.resultMemberInfo = resultMemberInfo;
    }

    public boolean isResultMemberCard()
    {
        return resultMemberCard;
    }

    public void setResultMemberCard(boolean resultMemberCard)
    {
        this.resultMemberCard = resultMemberCard;
    }

    public boolean isResultMemberCheckIn()
    {
        return resultMemberCheckIn;
    }

    public void setResultMemberCheckIn(boolean resultMemberCheckIn)
    {
        this.resultMemberCheckIn = resultMemberCheckIn;
    }

    public String getCustomId()
    {
        return customId;
    }

    public void setCustomId(String customId)
    {
        this.customId = customId;
    }

    public String getRank()
    {
        return rank;
    }

    public void setRank(String rank)
    {
        this.rank = rank;
    }

    public int getPoint()
    {
        return point;
    }

    public void setPoint(int point)
    {
        this.point = point;
    }

    public int getPoint2()
    {
        return point2;
    }

    public void setPoint2(int point2)
    {
        this.point2 = point2;
    }

    public boolean isExistMemberInfo()
    {
        return existMemberInfo;
    }

    public void setExistMemberInfo(boolean existMemberInfo)
    {
        this.existMemberInfo = existMemberInfo;
    }

    public String getErrorMessage()
    {
        return errorMessage;
    }

    public void setErrorMessage(String errorMessage)
    {
        this.errorMessage = errorMessage;
    }

    public int getErrorCode()
    {
        return errorCode;
    }

    public void setErrorCode(int errorCode)
    {
        this.errorCode = errorCode;
    }

    public DtoApCommon getApCommon()
    {
        return apCommon;
    }

    public void setApCommon(DtoApCommon apCommon)
    {
        this.apCommon = apCommon;
    }

    public boolean isResultMemberCardIssued()
    {
        return resultMemberCardIssued;
    }

    public void setResultMemberCardIssued(boolean resultMemberCardIssued)
    {
        this.resultMemberCardIssued = resultMemberCardIssued;
    }

    public int getGoodsCode()
    {
        return goodsCode;
    }

    public void setGoodsCode(int goodsCode)
    {
        this.goodsCode = goodsCode;
    }

    public int getGoodsPrice()
    {
        return goodsPrice;
    }

    public void setGoodsPrice(int goodsPrice)
    {
        this.goodsPrice = goodsPrice;
    }

}
