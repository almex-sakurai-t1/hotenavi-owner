package jp.happyhotel.owner;

import java.util.ArrayList;

/*
 * グループ店請求Formクラス
 */
public class FormOwnerBkoGroupBill
{
    private String             selContractID  = "";                      // 契約先ID
    private int                selHotelID     = 0;                       // ホテルID
    private int                selUserID      = 0;                       // ログインユーザID
    private int                selYear        = 0;
    private int                selMonth       = 0;
    private String             errMsg         = "";

    private ArrayList<Integer> idList         = new ArrayList<Integer>(); // ホテルID
    private ArrayList<String>  hotenaviIdList = new ArrayList<String>(); // ホテナビID

    private ArrayList<String>  hotelNameList  = new ArrayList<String>(); // ホテル名
    private ArrayList<String>  billNameList   = new ArrayList<String>(); // 法人名

    private ArrayList<Integer> incomeList     = new ArrayList<Integer>(); // 収入
    private ArrayList<Integer> outgoList      = new ArrayList<Integer>(); // 支出

    // 合計
    private int                sumIncome      = 0;
    private int                sumOutgo       = 0;

    public String getSelContractID()
    {
        return selContractID;
    }

    public void setSelContractID(String selContractID)
    {
        this.selContractID = selContractID;
    }

    public int getSelHotelID()
    {
        return selHotelID;
    }

    public void setSelHotelID(int selHotelID)
    {
        this.selHotelID = selHotelID;
    }

    public String getErrMsg()
    {
        return errMsg;
    }

    public void setErrMsg(String errMsg)
    {
        this.errMsg = errMsg;
    }

    public int getSelUserID()
    {
        return selUserID;
    }

    public void setSelUserID(int selUserID)
    {
        this.selUserID = selUserID;
    }

    public int getSelYear()
    {
        return selYear;
    }

    public void setSelYear(int selYear)
    {
        this.selYear = selYear;
    }

    public int getSelMonth()
    {
        return selMonth;
    }

    public void setSelMonth(int selMonth)
    {
        this.selMonth = selMonth;
    }

    public ArrayList<Integer> getIdList()
    {
        return idList;
    }

    public void setIdList(ArrayList<Integer> idList)
    {
        this.idList = idList;
    }

    public ArrayList<String> getHotenaviIdList()
    {
        return hotenaviIdList;
    }

    public void setHotenaviIdList(ArrayList<String> hotenaviIdList)
    {
        this.hotenaviIdList = hotenaviIdList;
    }

    public ArrayList<String> getHotelNameList()
    {
        return hotelNameList;
    }

    public void setHotelNameList(ArrayList<String> hotelNameList)
    {
        this.hotelNameList = hotelNameList;
    }

    public ArrayList<String> getBillNameList()
    {
        return billNameList;
    }

    public void setBillNameList(ArrayList<String> billNameList)
    {
        this.billNameList = billNameList;
    }

    public ArrayList<Integer> getIncomeList()
    {
        return incomeList;
    }

    public void setIncomeList(ArrayList<Integer> incomeList)
    {
        this.incomeList = incomeList;
    }

    public ArrayList<Integer> getOutgoList()
    {
        return outgoList;
    }

    public void setOutgoList(ArrayList<Integer> outgoList)
    {
        this.outgoList = outgoList;
    }

    public int getSumIncome()
    {
        return sumIncome;
    }

    public void setSumIncome(int sumIncome)
    {
        this.sumIncome = sumIncome;
    }

    public int getSumOutgo()
    {
        return sumOutgo;
    }

    public void setSumOutgo(int sumOutgo)
    {
        this.sumOutgo = sumOutgo;
    }
}
