package jp.happyhotel.common;

public class ConvertHexToInt
{

    /**
     * 16i”1Œ…‚©‚çint‚É•ÏŠ·
     * 
     * @param hex 16i”•¶š—ñ
     * @return int 10i”
     */
    public static int HexToInt(String hex)
    {
        int value = 0;
        char hexDigit[] = hex.toCharArray();

        value = (Character.digit( hexDigit[0], 16 ));
        return(value);
    }
}
